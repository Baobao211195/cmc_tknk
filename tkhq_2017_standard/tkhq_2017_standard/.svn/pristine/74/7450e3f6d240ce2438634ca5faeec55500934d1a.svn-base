package com.tkhq.cmc.dto;

public class BCNKHHCOUDDTO {
	private int quy;
	private int nam;
	private String ma_hang;
	private String ten_nuoc;
	private double trigia_db;
	private double tyle_db;
	private double trigia_tt;
	private double tyle_tt;
	private double trigia_kh;
	private double tyle_kh;
	public BCNKHHCOUDDTO(int quy, int nam, String ma_hang,
			String ten_nuoc, double trigia_db, double tyle_db,
			double trigia_tt, double tyle_tt, double trigia_kh, double tyle_kh) {
		super();
		this.quy = quy;
		this.nam = nam;
		this.ma_hang = ma_hang;
		this.ten_nuoc = ten_nuoc;
		this.trigia_db = trigia_db;
		this.tyle_db = tyle_db;
		this.trigia_tt = trigia_tt;
		this.tyle_tt = tyle_tt;
		this.trigia_kh = trigia_kh;
		this.tyle_kh = tyle_kh;
	}
	public int getQuy() {
		return quy;
	}
	public void setQuy(int quy) {
		this.quy = quy;
	}
	public int getNam() {
		return nam;
	}
	public void setNam(int nam) {
		this.nam = nam;
	}
	public String getMa_hang() {
		return ma_hang;
	}
	public void setMa_hang(String ma_hang) {
		this.ma_hang = ma_hang;
	}
	public String getTen_nuoc() {
		return ten_nuoc;
	}
	public void setTen_nuoc(String ten_nuoc) {
		this.ten_nuoc = ten_nuoc;
	}
	public double getTrigia_db() {
		return trigia_db;
	}
	public void setTrigia_db(double trigia_db) {
		this.trigia_db = trigia_db;
	}
	public double getTyle_db() {
		return tyle_db;
	}
	public void setTyle_db(double tyle_db) {
		this.tyle_db = tyle_db;
	}
	public double getTrigia_tt() {
		return trigia_tt;
	}
	public void setTrigia_tt(double trigia_tt) {
		this.trigia_tt = trigia_tt;
	}
	public double getTyle_tt() {
		return tyle_tt;
	}
	public void setTyle_tt(double tyle_tt) {
		this.tyle_tt = tyle_tt;
	}
	public double getTrigia_kh() {
		return trigia_kh;
	}
	public void setTrigia_kh(double trigia_kh) {
		this.trigia_kh = trigia_kh;
	}
	public double getTyle_kh() {
		return tyle_kh;
	}
	public void setTyle_kh(double tyle_kh) {
		this.tyle_kh = tyle_kh;
	}
	

}
