package com.tkhq.cmc.dto;

import com.tkhq.cmc.model.Tbs_Dvt;

public class Tbs_DvtDTO {
	private String maDvt;
    private String tenDvt;
    private String maStd;
    private Double tlQuydoi;
    
    public Tbs_DvtDTO(){}
    
    public Tbs_DvtDTO(Tbs_Dvt entity){
    	this.maDvt = entity.getMaDvt();
    	this.tenDvt = entity.getTenDvt();
    	this.maStd = entity.getMaStd();
    	this.tlQuydoi = entity.getTlQuydoi();
    }
    
    public String getMaDvt() {
		return maDvt;
	}
	public void setMaDvt(String maDvt) {
		this.maDvt = maDvt;
	}
	public String getTenDvt() {
		return tenDvt;
	}
	public void setTenDvt(String tenDvt) {
		this.tenDvt = tenDvt;
	}
	public String getMaStd() {
		return maStd;
	}
	public void setMaStd(String maStd) {
		this.maStd = maStd;
	}
	public Double getTlQuydoi() {
		return tlQuydoi;
	}
	public void setTlQuydoi(Double tlQuydoi) {
		this.tlQuydoi = tlQuydoi;
	}

}
