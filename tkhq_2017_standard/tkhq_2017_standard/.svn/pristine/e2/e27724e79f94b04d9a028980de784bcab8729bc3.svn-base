    z-index:1041 !important;    
     min-width:300px !important;

}
.popup-footer {
    background-color:#EDF1F5;
    border:none;
    /*padding: 20px 20px 5px 20px;*/ 
    padding: 20px 15px 15px 20px;   
    text-align: right;
}

