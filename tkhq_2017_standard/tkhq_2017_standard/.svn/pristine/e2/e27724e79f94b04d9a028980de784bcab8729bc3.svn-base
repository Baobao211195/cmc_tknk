/*Style Popup Start*/
.popup-dialog
{
    display:flex;
    margin-left:auto;
    margin-right:auto;
    /*margin-top:-450px !important;*/
    z-index:1061 !important;
     width:350px !important;
}

.popup-header
{
    /*background-color: #EDF1F5; #498eca;   #EDF1F5;*/
    border:none;
    height:40px;
    min-height: 16.428571429px;
    padding: 5px 20px 10px 10px;

    /*Thï¿½m*/
    background-color: #39b3d7;
}
.popup-title
{
    margin-top: 5px;
    margin-bottom: 10px;
}
.popup-header h3
{
    /*color:#0b4b7b;*/
     color:#FFF !important;
    font-size:16px !important;
    font-weight:bold;
    width:80%;
   /*  margin-left: 40px !important;
    margin-top: -32px;
 */

}
.popup-body {
    background-color:#EDF1F5;
    border:none;
    color:#000000;
    max-height: 450px;    
    /*max-width:350px !important;*/
    position: relative;
    padding: 10px 20px 10px 20px;        
}
.popup-content
{       
    background-color:#EDF1F5;
    border:1px solid #cccccc;
    border-radius:5px;
    -webkit-border-radius:5px;
    -moz-border-radius:5px;
    -ms-border-radius:5px;
