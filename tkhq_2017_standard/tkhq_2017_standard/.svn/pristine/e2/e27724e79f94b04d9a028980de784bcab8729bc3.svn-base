.icon-success
{    
    background:url(../images/icon-info.gif) no-repeat left 3px;
    min-height:32px;
    min-width:32px; 
    padding-left:45px;
    background-size:25px 25px;
}
.icon-warning
{
    
    background:url(../images/icon-warning.png) no-repeat left 2px;
    min-height:32px; /*32*/
    min-width:32px; 
    padding-left:45px;
    background-size:27px 22px;
}
.icon-error
{    
    background:url(../images/icon-error.png) no-repeat left 2px;
    min-height:32px;
    min-width:32px; 
    padding-left:45px;
    background-size:27px 25px;
}
.icon-confirm
{    
    background: url(../images/icon-question.gif) no-repeat left 3px;
    min-height:32px;
    min-width:32px; 
    padding-left:45px;
    background-size:25px 25px;
}
/*Style Popup End*/

div#loading
{
    display: none;
    width: 100%;
    height: 95%;
    position: absolute; /*background:url(Images/loading_spinner.gif) no-repeat center #fff;*/
    background-color: white;
    text-align: center;
    margin-left: 0px;
    font: normal 16px Tahoma, Geneva, sans-serif;
    border: none;
    opacity: 0.4;
    z-index: 9999 !important;
    overflow: hidden;       
    
}
div#loading::-webkit-scrollbar { width: 0 !important;}

div#loading img
{
    width: 100px;
    height: 100px;
    position: fixed;
    margin-left: -50px;
    margin-top: -50px;
    top: 50%;
    left: 50%;
}

/*Show Progress bar Start*/
.fade-in-out {
	transition: 1s linear all;  
	opacity: 0.6;
	z-index: 9999 !important;
	text-align: center;
	position: absolute;
	overflow: hidden;
	top: 0;
	left: 0;
	background: #5C5C5C;
	width: 100%;
	height: 1000px;
}

.fade-in-out.ng-hide {
  opacity: 0;
}

.fade-in-out .progressBar {
	width: 450px;
	top: 33%;
	position: absolute;
	left: 33%;
}
/*Show Progress bar End*/

/*Form search*/
legend{
	color: #449d44;
}

.paging-record{
	margin: 25px 0;
	font-size: 14px;
}

/*Dialog modal*/
.modal-header .modal-title{
	color: #01aef0;
}

.input-warning{
	color: red;
	margin-top: 5px;
}

.modal-body label{
	margin-top: 5px;
}

/*Table Data*/
.table-scroll{
	max-height: 500px;
	width: 100%;
	overflow-x: scroll;
	overflow-y: auto;
}

.table-border-right{
	border-right-width: 1px;
	border-right-style: solid;
	line-height: 1.5;
	border-right-color: gainsboro;
}

.table-text{
	text-align: center;
}

/*Message error*/
.messageErrorArea{
	font-size: 13px;
	color: red;
	margin-bottom: 10px;
}

/*Fill background min, max*/
.bg_red{
	background-color: red !important;
}

.bg_yellow{
	background-color: yellow !important;
}

.ky_null{
	min-width: 52px;
}

.slash{
	font-size: 24px;
	text-align: center;
	position: relative;
	top: -3px;
}