package com.tkhq.cmc.dto;

public class BCTTXNKTCCHQDTO {
	private int nam;
	private String ma_cuc;
	private String ten_cuc;
	private String ma_hq;
	private String ten_hq;
	private long so_luong_dn;
	private long so_luong_tk;
	private double trigia_xnk;
	public BCTTXNKTCCHQDTO(int nam, String ma_cuc, String ten_cuc,
			String ma_hq, String ten_hq, long so_luong_dn, long so_luong_tk,
			double trigia_xnk) {
		super();
		this.nam = nam;
		this.ma_cuc = ma_cuc;
		this.ten_cuc = ten_cuc;
		this.ma_hq = ma_hq;
		this.ten_hq = ten_hq;
		this.so_luong_dn = so_luong_dn;
		this.so_luong_tk = so_luong_tk;
		this.trigia_xnk = trigia_xnk;
	}
	public int getNam() {
		return nam;
	}
	public void setNam(int nam) {
		this.nam = nam;
	}
	public String getMa_cuc() {
		return ma_cuc;
	}
	public void setMa_cuc(String ma_cuc) {
		this.ma_cuc = ma_cuc;
	}
	public String getTen_cuc() {
		return ten_cuc;
	}
	public void setTen_cuc(String ten_cuc) {
		this.ten_cuc = ten_cuc;
	}
	public String getMa_hq() {
		return ma_hq;
	}
	public void setMa_hq(String ma_hq) {
		this.ma_hq = ma_hq;
	}
	public String getTen_hq() {
		return ten_hq;
	}
	public void setTen_hq(String ten_hq) {
		this.ten_hq = ten_hq;
	}
	public long getSo_luong_dn() {
		return so_luong_dn;
	}
	public void setSo_luong_dn(long so_luong_dn) {
		this.so_luong_dn = so_luong_dn;
	}
	public long getSo_luong_tk() {
		return so_luong_tk;
	}
	public void setSo_luong_tk(long so_luong_tk) {
		this.so_luong_tk = so_luong_tk;
	}
	public double getTrigia_xnk() {
		return trigia_xnk;
	}
	public void setTrigia_xnk(double trigia_xnk) {
		this.trigia_xnk = trigia_xnk;
	}
	
}
