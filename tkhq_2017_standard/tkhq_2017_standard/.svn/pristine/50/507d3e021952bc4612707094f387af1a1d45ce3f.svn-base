package com.tkhq.cmc.dto;

public class BCThayDoiDoTHQTNV {
	private String lydo;
	private Double giatri_cu;
	private Double giatri_moi;
	private Double thay_doi;
	public String getLydo() {
		return lydo;
	}
	public void setLydo(String lydo) {
		this.lydo = lydo;
	}
	public Double getGiatri_cu() {
		return giatri_cu;
	}
	public void setGiatri_cu(Double giatri_cu) {
		this.giatri_cu = giatri_cu;
	}
	public Double getGiatri_moi() {
		return giatri_moi;
	}
	public void setGiatri_moi(Double giatri_moi) {
		this.giatri_moi = giatri_moi;
	}
	public Double getThay_doi() {
		return thay_doi;
	}
	public void setThay_doi(Double thay_doi) {
		this.thay_doi = thay_doi;
	}
	public BCThayDoiDoTHQTNV(String lydo, Double giatri_cu, Double giatri_moi,
			Double thay_doi) {
		super();
		this.lydo = lydo;
		this.giatri_cu = giatri_cu;
		this.giatri_moi = giatri_moi;
		this.thay_doi = thay_doi;
	}
	public BCThayDoiDoTHQTNV() {
	}
	
}
